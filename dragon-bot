var Bot = require('dragon-bot'); var bot = new Bot({    email: <Tia.Schioppo@bayridgeprep.org>,    password: <Tia Tia Tia>}); 
.on(bot.triggers.command, 'speak')    .do(function( args ){ this.reply('roar');});
.on(bot.triggers.command, 'front flip')    .do(function( args ){ this.reply('dragon flips itself forward in the air');});
.on(bot.triggers.command, 'back flip')    .do(function( args ){ this.reply('dragon flips itself backward in the air');});
.on(bot.triggers.command, 'fire attack')    .do(function( args ){ this.reply('dragon breaths fire at intruder ');});
.on(bot.triggers.command, ' bro fist')    .do(function( args ){ this.reply('dragon puts one of his front feet to your fist');});   
.on(bot.triggers.command, ' sit')    .do(function( args ){ this.reply('dragon sits down at your feet');});    
.on(bot.triggers.command, 'lay down')    .do(function( args ){ this.reply('dragon plops down at your feet');});
